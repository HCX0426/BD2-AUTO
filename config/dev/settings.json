{
  "env": "dev",
  "framework": {
    "default_ocr_engine": "easyocr",
    "default_device_uri": "Windows:///?title_re=.*BrownDust.*",
    "default_task_timeout": 300,
    "default_text_fuzzy_match": true,
    "default_window_operation_delay": 0.0,
    "default_screenshot_delay": 0.0,
    "default_check_element_delay": 0.0,
    "default_click_delay": 0.0,
    "default_key_duration": 0.1,
    "default_base_resolution": [1920, 1080],
    "template_extensions": [".png", ".jpg", ".jpeg", ".bmp"]
  },
  "device": {
    "windows_app_title": "BD2-游戏窗口",
    "adb_path": "C:/platform-tools/adb.exe",
    "adb_device_ip": "192.168.1.100:5555"
  },
  "ocr": {
    "engine": "paddleocr",
    "confidence": 0.7,
    "tesseract_path": "C:/Tesseract-OCR/tesseract.exe"
  },
  "log": {
    "level": "DEBUG",
    "max_size": 10,
    "backup_count": 5
  },
  "task": {
    "timeout": 300,
    "retry_count": 3,
    "click_interval": 0.5
  },
  "debug": {
    "debug_mode": true,
    "skip_sleep": false
  }
}